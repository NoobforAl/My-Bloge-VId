<html>

<!-- ///======    ----Head tag----     -->

<head>
    <meta charset="UTF-8">

    <!-- ///======    ----BootStarp----     -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ho+j7jyWK8fNQe+A12Hb8AhRq26LrZ/JpcUGGOn+Y7RsweNrtN/tE3MoK7ZeZDyx" crossorigin="anonymous"></script>

    <!-- ///======    ----Title----     -->
    <title>Weather Dashboard</title>

</head>

<body>

    <!-- ///=====   ----Nav----   -->
    <nav class="navbar navbar-light bg-light justify-content-between">

        <!-- ///=====   ----Brand Web Href Index----   -->
        <a class="navbar-brand" href="/">Weather Dashboard</a>

        <!-- ///=====   ----Sapm!!----   -->
        <ul class="navbar-nav mr-auto">
            <li class="nav-item active">
                <a class="nav-link" href="#"><span class="sr-only"></span></a>
            </li>
            <li class="nav-item active">
                <a class="nav-link" href="#"><span class="sr-only"></span></a>
            </li>
            <li class="nav-item active">
                <a class="nav-link" href="#"><span class="sr-only"></span></a>
            </li>
        </ul>


        <!-- ///=====   ----Heder Serch!!----   -->
        <div class="form-inline">
            <input class="form-control mr-sm-2" id="serch_His" name="serch" type="search" placeholder="Search City" aria-label="Search">
            <button class="btn btn-outline-success my-2 my-sm-0" type="submit" onclick="serch(); saveHis();">Search</button>
        </div>

    </nav>
    <br/>




    <!-- ///=====   ----Body Weather----   -->
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-10">
                <div class="jumbotron">

                    <h1 class="display-4">
                        <!-- ///======    ----City Name----     -->
                        <samp id="nmae_cy"></samp>

                        <!-- ///======    ----Time Todey----     -->
                        <samp id="time_t"></samp>

                        <!-- ///======- ---Icon Weather---- -->
                        <samp id="icon_wa"></samp>
                    </h1>

                    <!-- ///======    ----Temp Kelvin Defical,Chang to C ----     -->
                    <p id="temp_c" class="lead"></p>

                    <!-- ///======    ----Huidity %----     -->
                    <p id="Hum_abr" class="lead"></p>

                    <!-- ///======    ----Wind Speed M/S Difecal----     -->
                    <p id="speed_h" class="lead"></p>

                    <!-- ///======    ----UV index location for Uv  ----     -->
                    <p id="UV_in" class="lead"></p>


                    <!-- ///======    ----5-Day Later!----     -->
                    <span id="brbr"></span>
                    <div class="row" id="5_Day"></div>
                </div>
            </div>


            <!-- ///======    ----History----     -->
            <div class="col-sm-2">
                <ul class="list-group">
                    <li class="list-group-item active">Serch Hestory</li>
                </ul>
                <ul id='hstory' class="list-group"></ul>
            </div>
        </div>
    </div>




    <!-- ///=====   ----Script For Serch----   -->
    <script src="/Serch.js"></script>


</body>

</html>